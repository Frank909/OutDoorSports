<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="outdoorapp.to.ManagerDiEscursione" table="tbl_mde" catalog="outdoorsports">
		<id name="idMde" type="java.lang.Integer">
			<column name="id_mde" />
			<generator class="increment" />
		</id>
		<property name="stipendio" type="double">
			<column name="stipendio" />
		</property>
	</class>
	
	<query name="getAllManagerDiEscursione">
		<![CDATA[ from outdoorapp.to.ManagerDiEscursione ]]></query>
		
</hibernate-mapping>