<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="outdoorapp.to.Partecipante" table="tbl_partecipante" catalog="outdoorsports">
		<id name="idPartecipante" type="java.lang.Integer">
			<column name="id_partecipante" />
			<generator class="increment" />
		</id>
		<property name="NTesseraSanitaria" type="string">
			<column name="n_tessera_sanitaria" not-null="true" />
		</property>
		<property name="certificatoSrc" type="string">
			<column name="certificato_src" not-null="true" />
		</property>
		<property name="dataCertificatoSrc" type="date">
			<column name="data_certificato_src" not-null="true" />
		</property>
	</class>
	
	<query name="getAllPartecipanti">
		<![CDATA[ from outdoorapp.to.Partecipante ]]></query>
</hibernate-mapping>